ter\n\nif app.get(\"env\") is \"development\"\n	app.use express.errorHandler({dumpExceptions:false, showStack:false})\n	app.disable('view cache')\nelse\n	#production error handling\n	app.use myErrorHandler.logErrors\n	app.use myErrorHandler.clientErrorHandler\n	app.use myErrorHandler.errorHandler\n\nwinston.log('info', 'Requiring Routes')\nrequire(myRoutes)(app, myMiddleware, session_store) \n\nwinston.log('info', 'Creating http server')\n# Step x - Create and listen server\nserver = http.createServer(app)\n\nwinston.log('info', 'Creating socket server')\nio = socket_io.listen(server)\n\nwinston.log('info', 'Configuring socket server')\nio.set(\"log level\", 0)\nrequire(mySocketRoutes)(app, myMiddleware, io, session_store) \n\nwinston.log('info', 'Ready')\nserver.listen app.get('port'), ->\n	console.log \"Express server listening on port \" + server.address().port + \", in \" +  app.get(\"env\")\n",
